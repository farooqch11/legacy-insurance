<p id="notice"><%= notice %></p>



<table>
  <thead>
  <tr>
    <th>Name</th>
    <th>Age</th>
    <th>Birthday</th>
    <th>Address</th>
    <th>Stateofbirth</th>
    <th>Cityofbirth</th>
    <th>Height</th>
    <th>Weight</th>
    <th>Licensenumber</th>
    <th>State</th>
    <th>Ssn</th>
    <th>Salary</th>
    <th>Agency</th>
    <th>Job</th>
    <th>Doh</th>
    <th colspan="3"></th>
  </tr>
  </thead>
  <tbody>
  <% @policies.each do |policy| %>
      <tr>
        <td><%= policy.Name %></td>
        <td><%= policy.age %></td>
        <td><%= policy.birthday %></td>
        <td><%= policy.address %></td>
        <td><%= policy.StateofBirth %></td>
        <td><%= policy.CityofBirth %></td>
        <td><%= policy.height %></td>
        <td><%= policy.weight %></td>
        <td><%= policy.Licensenumber %></td>
        <td><%= policy.State %></td>
        <td><%= policy.SSN %></td>
        <td><%= policy.Salary %></td>
        <td><%= policy.Agency %></td>
        <td><%= policy.Job %></td>
        <td><%= policy.Doh %></td>
        <td><%= link_to 'Show', policy %></td>
        <td><%= link_to 'Edit', edit_policy_path(policy) %></td>
        <td><%= link_to 'Destroy', policy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
  <% end %>
  </tbody>
</table>

