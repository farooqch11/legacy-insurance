<div class="top">
  <div class="top-bar">
    <div class="top-bar-title"><img src="http://i1167.photobucket.com/albums/q627/spawnreed/leg_zpsdn4xuf6y.png" class="logo"height="5" alt="" /></div>
    <div>
      <div class="top-bar-left"></div>
      <div class="top-bar-right">
      </div>
    </div>
  </div>
</div>
<body class='devise'>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <link href="https://fonts.googleapis.com/css?family=Josefin+Sans:300,300i,400,400i,600,600i,700,700i" rel="stylesheet">
    <div class='body'>
      <div class="sign-up-box">
        <div class="row">
          <div class="twelve columns center">
            <h5 class='h5'>Welcome Back!</h5>
            <form>
              <%= f.email_field :email, autofocus: true, placeholder: 'Email Address' %>
              <br>
              <%= f.password_field :password, autocomplete: "off", placeholder: 'Password' %>
              <br>
              <p>Don't have an account? <%= link_to "Sign up", new_user_registration_path %></p>
              <p>Forgot Password? <%= link_to "Forgot Password", edit_user_registration_path %></p>
              <div class="button-position">
                <button class="button-signup">Sign in</button>
                <div class="actions">
                  <%= f.submit "Sign in", class:'button-signup'  %>
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>
<% end %>
</body>
