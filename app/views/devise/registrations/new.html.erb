<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <div class="signup">
      <img class="bg-body" src="<%=image_url('img6.jpg')%>" alt="image description">
      <div class="signup-form" style="width: 400px;left: 54%;">
        <h2>SignUp</h2>
        <div id="error_explanation">
          <ul>
            <% @user.errors.full_messages.each do |msg| %>
                <li><%=msg%></li>
            <% end %>
          </ul>
        </div>
        <div class="hold">

          <!--<div class="row"><input type="text" placeholder="Your name"></div>-->
          <div class="row"><%= f.email_field :email, autofocus: true, placeholder: 'Email Address' %></div>
          <div class="row"><%= f.password_field :password, autocomplete: "off", placeholder: 'Password' %></div>
          <div class="row"><%= f.password_field :password_confirmation, autocomplete: "off", placeholder: 'Confirm Password' %></div>
          <div class="row add"><%= f.submit "Sign up", class:'button-signup'  %></div>
          <div class="note">Already have an account?<a href="<%=new_user_session_path%>">SignIn</a></div>
        </div>
      </div>
    </div>
<%end%>
